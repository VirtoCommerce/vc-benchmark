      errorCode
      errorMessage
      errorParameters {
        key
        value
      }
      objectId
    }
    availablePaymentMethods {
      code
      logoUrl
      price {
        ...moneyFields
      }
    }
    availableShippingMethods {
      id
      code
      logoUrl
      optionName
      optionDescription
      price {
        ...moneyFields
      }
    }
    gifts {
      id
      imageUrl
      name
      lineItemId
    }
    availableGifts {
      id
      imageUrl
      name
      lineItemId
    }
  }
}

