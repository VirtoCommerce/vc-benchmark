    payments {
      id
      paymentGatewayCode
      billingAddress {
        ...cartAddressFields
      }
    }
    addresses {
      ...cartAddressFields
    }
    items {
      ...lineItemFields
    }
    currency {
      ...currencyFields
    }
    total {
      ...moneyFields
    }
    discountTotal {
      ...moneyFields
    }
    subTotal {
      ...moneyFields
    }
    shippingTotal {
      ...moneyFields
    }
    shippingPrice {
      ...moneyFields
    }
    taxTotal {
      ...moneyFields
    }
    extendedPriceTotal {
      ...moneyFields
    }
    extendedPriceTotalWithTax {
      ...moneyFields
    }
